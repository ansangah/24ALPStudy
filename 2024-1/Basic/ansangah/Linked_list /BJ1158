#include <iostream>
#include <queue>
using namespace std;

int main() {
	int n,k;
	cin >> n >> k;
	
	queue<int> q;
	for (int i = 1; i <= n; i++)
        q.push(i);

	cout << "<";
	while (q.size() > 1) 
    {
		for (int i = 1; i < k; i++) 
        {
			int tmp = q.front();
            // 1부터 n까지 큐에 삽입
			q.pop(); // 앞에서 빼서
			q.push(tmp); // 뒤에 다시 넣는다
		}
		cout << q.front() << ", ";
		q.pop(); // 출력된 요소 큐에서 삭제
	}
	cout << q.front() << ">\n";
}
